[bundles]
lib/adblockplus.js =
  adblockpluscore/lib/filterListener.js
  adblockpluscore/lib/synchronizer.js
  lib/devtools.js
  lib/requestBlocker.js
  lib/popupBlocker.js
  lib/subscriptionInit.js
  lib/filterComposer.js
  lib/stats.js
  lib/uninstall.js
  lib/csp.js
  lib/contentFiltering.js
