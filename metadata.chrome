[general]
basename = adblockpluschrome
version = 3.4.3
author = eyeo GmbH
backgroundScripts = polyfill.js
  ext/common.js
  ext/background.js
  lib/compat.js
  lib/adblockplus.js
testScripts = ../polyfill.js
  ../ext/common.js
  ../ext/background.js
  ../lib/compat.js
  tests.js
options = options.html
browserAction =
  icons/abp-16.png
  icons/abp-20.png
  icons/abp-32.png
  icons/abp-40.png
  popup.html
icons =
  icons/abp-16.png
  icons/abp-32.png
  icons/detailed/abp-48.png
  icons/detailed/abp-64.png
  icons/detailed/abp-128.png

[manifest]
devtools_page = devtools.html
storage.managed_schema = managed-storage-schema.json
permissions =
  tabs
  <all_urls>
  contextMenus
  webRequest
  webRequestBlocking
  webNavigation
  storage
  unlimitedStorage
  notifications

[compat]
chrome = 49.0
opera = 36.0

[contentScripts]
document_start =
  polyfill.js
  ext/common.js
  ext/content.js
  include.preload.js
document_end = include.postload.js

[mapping]
subscriptions.json = adblockpluscore/data/subscriptions.json
data/languages.json = adblockplusui/data/languages.json
first-run.html = adblockplusui/first-run.html
common.js = adblockplusui/common.js
